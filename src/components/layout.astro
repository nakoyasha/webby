---
import WipFeature from "./wipFeature.astro";
import wipFeature from "./wipFeature.astro";
const { isWIPFeature } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Haruka's silly little website on the interwebs</title>
    <link rel="stylesheet" href="/style/fonts.css" />
    <link rel="stylesheet" href="/style/animation.css" />
    <link rel="stylesheet" href="/style/stickers.css" />
    <link rel="stylesheet" href="/style/meta.css" />

    <link rel="icon" href="/assets/favicon.png" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.9.0/styles/atom-one-dark.min.css"
    />
    <script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
    ></script>
    <link href="/style/bootstrap.min.css" rel="stylesheet" />
    <!-- <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous"> -->
    <script is:inline src="/js/bootstrap.min.js"></script>
    <script defer src="/scripts/meta.js"></script>
    <script
      defer
      src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.9.0/highlight.min.js"
    ></script>
  </head>

  <body>
    <div class="full-page gotham-font">
      {isWIPFeature && <WipFeature />}
      <slot />
    </div>
  </body>
</html>
