<p class="page-title">trackers/discord/builds</p>
<%- include(`../../../components/wip-feature`) %>

<div class="center-div vertical-center-thing topbar-margin build-list-gap">
  <h2>Latest builds</h2>
  <div class="build-list">
    <a class="make-bold-tag-fancy-pls blog-top build-list-build menhera-outline" href=/trackers/discord/meow>
      <div class="build-indicators">
        <div class="tooltip build-indicator build-indicator-canary">
          <span class="tooltiptext">This is the latest canary build</span>
        </div>
      </div>
      <div class="build-info">
        <h3><b>meow</h3></b>
        <p class=" blog-description">Saved on meow</p>
      </div>
    </a>

    <a class="make-bold-tag-fancy-pls blog-top build-list-build menhera-outline" href=/trackers/discord/meow>
      <div class="build-indicators">
        <div class="tooltip build-indicator build-indicator-stable">
          <span class="tooltiptext">This is the latest stable build</span>
        </div>

      </div>
      <div class="build-info">
        <h3><b>meow</h3></b>
        <p class=" blog-description">Saved on meow</p>
      </div>
    </a>
  </div>

  <h3>All builds</h3>
  <div class="build-list">
    <% builds.forEach((build)=>{ %>
    <a class="make-bold-tag-fancy-pls blog-top build-list-build menhera-outline" href=/trackers/discord/<%=build.name%>>
      <div class="build-indicators">
        <div class="tooltip build-indicator build-indicator-canary">
          <span class="tooltiptext">This is the latest canary build</span>
        </div>

        <div class="tooltip build-indicator build-indicator-stable">
          <span class="tooltiptext">This is the latest stable build</span>
        </div>

        <div class="tooltip build-indicator build-indicator-ptb">
          <span class="tooltiptext">This is the latest ptb build</span>
        </div>
      </div>

      <div class="build-info">
        <h3><b><%= build.title %></h3></b>
        <p class=" blog-description">Saved on <%=build.date%></p>
      </div>
    </a>
    <% }) %>
  </div>
</div>